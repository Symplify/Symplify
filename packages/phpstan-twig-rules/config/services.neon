services:
    - Symplify\PHPStanTwigRules\Templates\RenderTemplateWithParametersMatcher
    - Symplify\PHPStanTwigRules\NodeAnalyzer\SymfonyRenderWithParametersMatcher

    - Symplify\SmartFileSystem\SmartFileSystem

    # simple name resolver
    -
         class: Symplify\Astral\Naming\SimpleNameResolver
         factory: ['Symplify\Astral\StaticFactory\SimpleNameResolverStaticFactory', 'create']

    - Symplify\PackageBuilder\Reflection\PrivatesAccessor
    - Symplify\Astral\NodeValue\NodeValueResolver
    - Symplify\PackageBuilder\Php\TypeChecker
    - Symplify\Astral\NodeFinder\SimpleNodeFinder
    - PhpParser\NodeFinder

    # node analyzer
    - Symplify\PHPStanRules\NodeAnalyzer\ArrayAnalyzer
    - Symplify\PHPStanRules\Nette\NodeAnalyzer\TemplateRenderAnalyzer
    - Symplify\PHPStanRules\Nette\NodeAnalyzer\NetteTypeAnalyzer
    - Symplify\PHPStanRules\TypeAnalyzer\ObjectTypeAnalyzer
    - Symplify\PHPStanRules\TypeAnalyzer\TypeUnwrapper
    - Symplify\PHPStanRules\NodeAnalyzer\PathResolver
    # @todo move to template package
    - Symplify\PHPStanRules\ErrorSkipper
    - Symplify\PHPStanRules\Nette\TypeAnalyzer\ComponentMapResolver
    - Symplify\PHPStanRules\TypeAnalyzer\ClassMethodTypeAnalyzer
