services:
    - Symplify\CodingStandard\PhpParser\NodeNameResolver

    -
        class: Symplify\CodingStandard\Rules\PreferredSmartFileSystemOverNativeFileSystemFunctionCallRule
        tags: [phpstan.rules.rule]
        arguments:
            funcCallToPrefferedStaticCalls:
                preg_match: ['Nette\Utils\Strings', 'match']
                preg_matchAll: ['Nette\Utils\Strings', 'match']
                preg_replace: ['Nette\Utils\Strings', 'replace']
                preg_split: ['Nette\Utils\Strings', 'split']
                substr: ['Symplify\CodingStandard\Tests\Rules\PreferredSmartFileSystemOverNativeFileSystemFunctionCallRule\Fixture\SkipSelfCall', 'run']
