{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "friendsofphp/php-cs-fixer": "^3.4",
        "symplify/symplify-kernel": "^10.1",
        "symplify/package-builder": "^10.1",
        "symplify/autowire-array-parameter": "^10.1",
        "symplify/rule-doc-generator-contracts": "^10.1"
    },
    "require-dev": {
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "doctrine/orm": "^2.10",
        "symplify/symplify-kernel": "^10.1",
        "symplify/smart-file-system": "^10.1",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symplify/easy-coding-standard": "^10.1",
        "phpunit/phpunit": "^9.5",
        "symplify/rule-doc-generator": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<10.0.6",
        "symplify/composer-json-manipulator": "<10.0.6",
        "symplify/easy-coding-standard": "<10.0.6",
        "symplify/phpstan-rules": "<10.0.6",
        "symplify/easy-testing": "<10.0.6",
        "symplify/symfony-php-config": "<10.0.6",
        "symplify/simple-php-doc-parser": "<10.0.6",
        "symplify/php-config-printer": "<10.0.6",
        "symplify/console-color-diff": "<10.0.6",
        "symplify/markdown-diff": "<10.0.6",
        "symplify/amnesia": "<10.0.6",
        "symplify/phpstan-extensions": "<10.0.6",
        "symplify/rule-doc-generator": "<10.0.6",
        "symplify/vendor-patches": "<10.0.6",
        "symplify/skipper": "<10.0.6",
        "symplify/smart-file-system": "<10.0.6",
        "symplify/symfony-static-dumper": "<10.0.6",
        "symplify/git-wrapper": "<10.0.6",
        "symplify/monorepo-builder": "<10.0.6",
        "symplify/config-transformer": "<10.0.6",
        "symplify/easy-ci": "<10.0.6",
        "symplify/latte-phpstan-compiler": "<10.0.6",
        "symplify/template-phpstan-compiler": "<10.0.6",
        "symplify/phpstan-latte-rules": "<10.0.6",
        "symplify/easy-parallel": "<10.0.6",
        "symplify/neon-config-dumper": "<10.0.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
